# AI Handler (main response generation)
AI_PROVIDER=anthropic
AI_MODEL=claude-3-5-haiku-20241022

# Intent Classifier (LLM fallback) - Optional, can use different provider/model
INTENT_PROVIDER=anthropic
INTENT_MODEL=claude-3-5-haiku-20241022

# Anthropic API Key - Get at https://console.anthropic.com
ANTHROPIC_API_KEY=your-anthropic-key-here

# OpenAI API Key - Get at https://platform.openai.com
# OPENAI_API_KEY=your-openai-key-here

# Ollama Base URL - Optional, defaults to http://localhost:11434
# OLLAMA_BASE_URL=http://localhost:11434

# Example: Use different providers for different tasks
# AI_PROVIDER=anthropic
# AI_MODEL=claude-3-5-sonnet-20241022
# INTENT_PROVIDER=openai
# INTENT_MODEL=gpt-4o-mini
# ANTHROPIC_API_KEY=your-anthropic-key
# OPENAI_API_KEY=your-openai-key
